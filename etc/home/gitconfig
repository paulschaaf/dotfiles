[core]
	editor = $EDITOR -nw
[mergetool]
	keepbackup = false
[core]
    excludesfile = ~/.gitignore
	autocrlf = input
[user]
	name = Paul Schaaf
	email = paul.schaaf@gmail.com
[merge]
	tool = bc
[push]
	default = current
[mergetool "bc3"]
	trustExitCode = true
[credential]
	helper = keychain
[alias]
	squash = "!f(){ git reset --soft HEAD~${1} && git commit --edit -m\"$(git log --format=%B --reverse HEAD..HEAD@{1})\"; };f"
	co = checkout
	issue = "!f(){ echo ${GIT_BRANCH##*/} | sed \"s~_.*~~\"; };f"

