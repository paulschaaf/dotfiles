# ======== Setup Environment
setenv inScreen true
unsetenv SHLVL
caption always "%{= wk}%-Lw%{= bw}%n%f %t%{-}%+Lw %{= wk}%="
defmonitor on
defscrollback 1000
shell /bin/zsh
shelltitle '$ |-'
startup_message off
hardstatus on
hardstatus lastline
hardstatus string "%{=br yk}%m/%d %c %{= dd} %-=%{-} %H %l"
# ======== Switch Between Windows
bindkey "OP" select 1	# F1: goto window 1
bindkey "OQ" select 2	# F2: goto window 2
bindkey "OR" select 3	# F3: goto window 3
bindkey "OS" select 4	# F4: goto window 4
bindkey "[15~" select 5	# F5: goto window 5
bindkey "[23~" screen	# F11: new screen
bindkey "[24~" stuff "exit^M"	# F12: exit
bindkey "[1;3D" prev	# Alt-left:  prev window
bindkey "[1;3C" next	# Alt-right: next window
bind o focus
# ======== Other Keys
bindkey "OI" stuff "^Upopd^M"	# clear line then popd
bindkey "" windowlist	# Win key shows list of windows
bindkey "[21;2~" screen -t "Connect to?" 39 $HOME/bin/menu-rlogin.bash	# Menu key
# ======== Create Some Windows
screen -t '>'     0 cmd /T:1e
chdir $HOME/etc/env
screen -t '$|env:' 2
chdir 
screen 3
screen 1

