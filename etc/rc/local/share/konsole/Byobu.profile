[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=WhiteOnBlack
Font=Fira Code Retina,10,-1,5,53,0,0,0,0,0,Regular
UseFontLineChararacters=true

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Command=/usr/bin/byobu new-session -A -s defaultxscreen
Directory=/home/pschaaf
Environment=TERM=xterm,SHLVL=
Name=Byobu
Parent=FALLBACK/
StartInCurrentSessionDir=false
TerminalColumns=100
TerminalRows=24

[Interaction Options]
AutoCopySelectedText=true
MiddleClickPasteMode=0
TrimTrailingSpacesInSelectedText=true

[Keyboard]
KeyBindings=paul

[Scrolling]
HistoryMode=1
ScrollBarPosition=1
ScrollFullPage=true
